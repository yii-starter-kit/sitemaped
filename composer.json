{
    "name": "trntv/sitemaped",
    "description": "PHP Sitemap abstraction library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eugene Terentev",
            "email": "eugene@terentev.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sitemaped\\": "src/"
        }
    },
    "require": {
        "ext-dom": "@stable",
        "php": ">=7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.5"
    },
    "suggest": {
        "ext-zlib": "@stable"
    },
    "scripts": {
        "test": [
          "./vendor/bin/phpunit --configuration phpunit.xml --coverage-text"
        ]
    }
}
